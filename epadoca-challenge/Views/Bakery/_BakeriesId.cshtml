@model List<BakeryModel>

<table class="tableModal">
    <thead class="thead">
        <tr>
            <th scope="col">Id</th>
        </tr>
        <tr>
            <th scope="col">Nome</th>
        </tr>
        <tr>
            <th scope="col">Descrição</th>
        </tr>
        <tr>
            <th scope="col">Estado</th>
        </tr>
        <tr>
            <th scope="col">Cidade</th>
        </tr>
        <tr>
            <th scope="col">Rua</th>
        </tr>

    </thead>
    <tbody class="tbodyModal">

        @foreach (var bakery in Model)
        {
            @if (Model != null && Model.Any())
            {

                <tr>
                    <th>@bakery.Id</th>
                </tr>
                <tr>
                    <th>@bakery.Name</th>
                </tr>
                <tr>
                    <th>@bakery.Description</th>
                </tr>
                <tr>
                    <th>@bakery.State</th>
                </tr>
                <tr>
                    <th>@bakery.City</th>
                </tr>
                <tr>
                    <th>@bakery.Street</th>
                </tr>
            }
        }

    </tbody>

</table>