<!--  Bakery info Modal -->
<div id="modalBakeryInfo" class="modal" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Descrição da padaria</h5>
                <button type="button" class="btn-close" data-dismiss="modal" aria-label="Close">Sair</button>
            </div>
            <div class="modal-body">
                <table class="tableModal">
                    <thead class="thead">
                        <tr>
                            <th scope="col">Id</th>
                        </tr>
                        <tr>
                            <th scope="col">Nome</th>
                        </tr>
                        <tr>
                            <th scope="col">Descrição</th>
                        </tr>
                        <tr>
                            <th scope="col">Estado</th>
                        </tr>
                        <tr>
                            <th scope="col">Cidade</th>
                        </tr>
                        <tr>
                            <th scope="col">Rua</th>
                        </tr>

                    </thead>
                    <tbody class="tbodyModal">
                        <tr>
                            <th>1</th>
                        </tr>
                        <tr>
                            <th>Pandany</th>
                        </tr>
                        <tr>
                            <th>Fazendo sua vida ser mais doce</th>
                        </tr>
                        <tr>
                            <th>PE</th>
                        </tr>
                        <tr>
                            <th>OLINDA</th>
                        </tr>
                        <tr>
                            <th>TEREZITA BANDEIRA</th>
                        </tr>

                    </tbody>

                </table>

            </div>
        </div>
    </div>
</div>
<!--/////////////////////////////////////////////////////////////////////-->


<!--  New Bakery Modal -->
<div id="newBakeryModal" class="modal" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Descrição da padaria</h5>
                <button type="button" class="btn-close" data-dismiss="modal" aria-label="Close">Sair</button>
            </div>
            <div class="modal-body">
                <form class="formModal"> 
                    <div class="divForm mb-3">
                        <label for="exampleInputPassword1" class="form-label">Nome:</label>
                        <input type="password" class="form-control" id="exampleInputPassword1">
                    </div>
                    <div class="divForm mb-3">
                        <label for="exampleInputPassword1" class="form-label">Descrição:</label>
                        <input type="password" class="form-control" id="exampleInputPassword1">
                    </div>
                    <div class="divForm mb-3">
                        <label for="exampleInputPassword1" class="form-label">Estado:</label>
                        <input type="password" class="form-control" id="exampleInputPassword1">
                    </div>
                    <div class="divForm mb-3">
                        <label for="exampleInputPassword1" class="form-label">Cidade:</label>
                        <input type="password" class="form-control" id="exampleInputPassword1">
                    </div>
                    <div class="divForm mb-3">
                        <label for="exampleInputPassword1" class="form-label">Rua:</label>
                        <input type="password" class="form-control" id="exampleInputPassword1">
                    </div>

                    <button type="submit" class="btnForm btn-primary">Cadastrar</button>
                </form>

            </div>
        </div>
    </div>
</div>
